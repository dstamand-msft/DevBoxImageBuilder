{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch AIB Script",
            "type": "PowerShell",
            "request": "launch",
            "script": "${workspaceFolder}/IaC/MainRunner.ps1",
            "args": [
                "-SubscriptionId <value>",
                "-ResourceGroupName <value>",
                "-StorageAccountName <value>",
                "-AIBTemplateBicepTemplatePath <value>",
                "-AIBTemplateBicepParametersPath <value>",
                "-VMImageOffer <value>",
                "-VMImageSku <value>",
                "-WithManagedIdentity $false"
            ]
        }
    ]
}